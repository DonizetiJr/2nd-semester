#include <stdio.h>
#include <utils.h>

int main(int argc, char *argv[]) {
  int debug;
  char *s1, *s2;

  s1 = readLine();
  s2 = readLine();

  debug = strlen(s1);
  printf("s1 size = %d\n", debug);
  debug = strlen(s1);
  printf("s2 size = %d\n", debug);

  erase(s1);
  erase(s2);
  
  return 0;
}
